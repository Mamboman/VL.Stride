shader NoiseSDF : SDF3D, HappyNoise // general 3 in 1 out noise test, not really an SDF, but want to try importing noise lib
{
    compose ComputeFloat Frequency;
    compose ComputeFloat3 DomainOffset;
    compose ComputeFloat Amplitude;
    compose ComputeFloat Center;


    override float Compute(float3 p)
    {
        return perlin(p * Frequency.Compute() + DomainOffset.Compute()) * Amplitude.Compute() + Center.Compute();
    }
};