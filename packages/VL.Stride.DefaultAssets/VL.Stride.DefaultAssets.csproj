<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>$(AssemblyName)</PackageId>
    <Description>$(AssemblyName)</Description>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>stride_package_logo.png</PackageIcon>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Stride.Engine" Version="4.0.0.1-vvvv-0955" />
    <PackageReference Include="Stride.Video" Version="4.0.0.1-vvvv-0955" />
    <PackageReference Include="Stride.Physics" Version="4.0.0.1-vvvv-0955" />
    <PackageReference Include="Stride.Navigation" Version="4.0.0.1-vvvv-0955" />
    <PackageReference Include="Stride.Particles" Version="4.0.0.1-vvvv-0955" />
    <PackageReference Include="Stride.UI" Version="4.0.0.1-vvvv-0955" />
    <PackageReference Include="Stride.Core.Assets.CompilerApp" Version="4.0.0.1-vvvv-0955" IncludeAssets="build;buildTransitive" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Resources\" />
  </ItemGroup>
  <Target Name="IncludeStridePackageContent" BeforeTargets="InferPackageContents" DependsOnTargets="_StridePrepareAssetsForPack">
    <Message Importance="high" Text="Called Stride package targets" />
  </Target>
  <ItemGroup>
    <PackageReference Update="Nerdbank.GitVersioning" Version="2.3.186">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <None Include="..\stride_package_logo.png">
      <Pack>True</Pack>
      <PackagePath>
      </PackagePath>
    </None>
  </ItemGroup>
</Project>