<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <PackageId>$(AssemblyName)</PackageId>
    <Description>$(AssemblyName)</Description>
    <Description>Allows to use Stride as library in VL.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>stride_package_logo.png</PackageIcon>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\VL.Stride.EffectLib\src\VL.Stride.EffectLib.csproj" />
    <ProjectReference Include="..\..\VL.Stride.Windows\src\VL.Stride.Windows.csproj" />
    <ProjectReference Include="..\..\VL.Stride\src\VL.Stride.csproj" />
    <ProjectReference Include="..\..\StrideLiveEditor\XenkoLiveEditor\StrideLiveEditor.csproj" ReferenceOutputAssembly="false" />
  </ItemGroup>
  <ItemGroup Condition="'$(SolutionName)' == 'VL'">
    <ProjectReference Include="..\..\..\..\..\vvvv50\VL.Lang\src\VL.Lang.csproj" />
  </ItemGroup>
  <ItemGroup Condition="'$(SolutionName)' != 'VL'">
    <PackageReference Include="VL.Lang" Version="2020.2.0-0004-g36c2100357" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Update="Nerdbank.GitVersioning" Version="2.3.186">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\stride_package_logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>