<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <PackageId>$(AssemblyName)</PackageId>
    <Description>$(AssemblyName)</Description>
    <Description>Allows to use Stride as library in VL.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\VL.Stride.EffectLib\src\VL.Stride.EffectLib.csproj" />
    <ProjectReference Include="..\..\VL.Stride.Windows\src\VL.Stride.Windows.csproj" />
    <ProjectReference Include="..\..\VL.Stride\src\VL.Stride.csproj" />
    <ProjectReference Include="..\..\StrideLiveEditor\XenkoLiveEditor\StrideLiveEditor.csproj" ReferenceOutputAssembly="false" />
  </ItemGroup>
  <ItemGroup Condition="'$(SolutionName)' == 'VL'">
    <ProjectReference Include="..\..\..\..\..\vvvv50\VL.UI.Forms\VL.UI.Forms.csproj" />
  </ItemGroup>
  <ItemGroup Condition="'$(SolutionName)' != 'VL'">
    <PackageReference Include="VL.UI.Forms" Version="2020.1.0" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Update="Nerdbank.GitVersioning" Version="2.3.186">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>
</Project>